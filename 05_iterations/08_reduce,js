const myNum = [1,2,3,4,5]

const total = myNum.reduce(function (acc,curr){
    // console.log(`acc = ${acc} curr = ${curr}`);
    
    return acc+curr;
},0)

const newtotal = myNum.reduce((acc,curr)=>(
    // console.log(`acc = ${acc} curr = ${curr}`)
    
     acc+curr
),0)

console.log(newtotal);
